    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "120",
        "--experimental"
    ],
    "cpp.creator.headerFileContentPreset": "#ifndef {{*CLASSNAMEUPPER*}}_HPP\n#define {{*CLASSNAMEUPPER*}}_HPP\n\n// EXTERNOS\n\n// INTERNOS\n#include \"libutils/infof.h\"\n#include \"libutils/types/types.h\"\n\n// PROYECTO\n\nclass C{{*CLASSNAMECAPI*}}\n{\npublic:\n    C{{*CLASSNAMECAPI*}}();\n    ~C{{*CLASSNAMECAPI*}}();\n\nprivate:\n\n};\n\n#endif // {{*CLASSNAMEUPPER*}}_HPP",
    "cpp.creator.setPath": "true",
    "cpp.creator.sourceFileContentPreset": "#include \"{{*HEADERFILENAME*}}\"\n\n// EXTERNOS\n\n// INTERNOS\n\n// PROYECTO\n\nC{{*CLASSNAMECAPI*}}::C{{*CLASSNAMECAPI*}}()\n{\n\n}\n\nC{{*CLASSNAMECAPI*}}::~C{{*CLASSNAMECAPI*}}()\n{\n\n}",
    "cpp.creator.headerFileNamePreset": "{{*CLASSNAME*}}.hpp",
    "diffEditor.ignoreTrimWhitespace": false,
