    "editor.fontLigatures": false,
    "window.commandCenter": true,
    "taskExplorer.enabledTasks": {
        "ant": false,
        "bash": false,
        "batch": false,
        "composer": false,
        "gradle": false,
        "grunt": false,
        "gulp": false,
        "make": false,
        "maven": false,
        "npm": false,
        "perl": false,
        "pipenv": false,
        "powershell": false,
        "python": false,
        "ruby": false,
        "tsc": false
    },
    "taskExplorer.enableSideBar": true,
    "taskExplorer.groupMaxLevel": 4,
    "taskExplorer.debug": true,
    "taskExplorer.showExecuteWithArgumentsButton": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "task.problemMatchers.neverPrompt": false,
    "[cpp]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "sonarlint.focusOnNewCode": true,
    "c-cpp-flylint.clang.enable": false,
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.lizard.enable": false,
    "python.analysis.autoFormatStrings": true,
    "black-formatter.args": [
        "--line-length=150"
    ],
    // "sonarlint.pathToCompileCommands": "/path/to/your/compile/compile_commands.json",
    // "editor.rulers": [
    //     100,
    //     120
    // ],
    "editor.mouseWheelZoom": true,
    "remote.autoForwardPortsSource": "hybrid",
    "editor.lineHeight": 1.5,
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "editor.formatOnSave": true,
    "clang-format.executable": "",
    "cmake.showOptionsMovedNotification": false,
    "clang-format.assumeFilename": ".clang-format",
    "editor.defaultFormatter": "xaver.clang-format",
    // Change to your ssh config file "remote.SSH.configFile": "C:/Users/user/.ssh/config",
    "[properties]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "scm.defaultViewMode": "tree",
    "[groovy]": {
        "editor.defaultFormatter": "NicolasVuillamy.vscode-groovy-lint"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    // "sonarlint.rules": {
    //     "cpp:S1135": {
    //         "level": "off"
    //     }
    // },
    // "sonarlint.connectedMode.connections.sonarqube": [
    //     {
    //         "serverUrl": "your ip server",
    //         "connectionId": "your connection id"
    //     }
    // ],
    "cmake.parallelJobs": 10,
    "clangd.arguments": [
        "-header-insertion=never"
    ],
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "git.replaceTagsWhenPull": true,
}